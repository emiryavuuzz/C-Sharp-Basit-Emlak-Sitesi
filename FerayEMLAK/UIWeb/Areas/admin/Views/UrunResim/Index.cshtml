<form action="/admin/UrunResim/Index" method="post" enctype="multipart/form-data">
<h1 style="text-align:center;"><b> Çoklu Resim Yükleme</b></h1><br />
<br />



<input  style="margin:40px 0 0 100px;" type="file" name="files" multiple /><br />
<br />
<input style="float:right; margin:250px 30px 0 0; width:400px; height:50px;" type="submit" value="Kaydet"/>


</form>
@if (ViewBag.Message != null)
    {
        <div>
<span style="border:solid; border:2px; border-inline-color:green;">@ViewBag.Message</span>
</div>
    }
    else if(ViewBag.Error != null)
    {
               <div>
<span style="border:solid; border:2px;  border-color:darkgray; background-color:red; color:white;">@ViewBag.Error</span>
</div>
    }